{
  "manifest_version": 2,
  "name": "ACF Tools",
  "version": "2.4.2",
  "description": "Advanced Custom Fields code made simple!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*/wp-admin/*", "*://*/**/wp-admin/*"],
      "css": ["styles/style.css"],
      "js": [
        "lib/jquery-3.4.1.min.js",
        "scripts/app.js",
        "scripts/misc.js",
        "scripts/fields.js"
      ]
    }
  ]
}
